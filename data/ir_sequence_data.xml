<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_maintenance_plan" model="ir.sequence">
            <field name="name">Código de Planes de Mantenimiento</field>
            <field name="code">maintenance.plan.default</field>
            <field name="prefix">PLM</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="seq_maintenance_request_auto" model="ir.sequence">
            <field name="name">Código de Peticiones de Mantenimiento</field>
            <field name="code">maintenance.request.default</field>
            <field name="prefix">PEM</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
